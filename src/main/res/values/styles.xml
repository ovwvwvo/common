<?xml version="1.0" encoding="UTF-8" ?>
<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="ResourceName">

    <style name="Text_Large_Black">
        <item name="android:textSize">@dimen/text_size_normal</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="Text_Mid_Black">
        <item name="android:textSize">@dimen/text_size_small_1</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="Text_Small_Black">
        <item name="android:textSize">@dimen/text_size_small_2</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="Text_Large_White">
        <item name="android:textSize">@dimen/text_size_normal</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="Text_Mid_White">
        <item name="android:textSize">@dimen/text_size_small_1</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="Text_Small_White">
        <item name="android:textSize">@dimen/text_size_small_2</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="Theme.Dialog.Progressbar" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:gravity">center</item>
        <item name="android:windowAnimationStyle">@null</item>
    </style>

</resources>
